              </div>
            
          </div>
        </div>
        <p>
          <h:commandButton rendered="#{(sessionScope.user_role == 'P' || sessionScope.user_role == 'B') and userAction.ridePicker ==2}" styleClass="btn btn-large btn-success" value="Register" action="#{userAction.registerRide}">

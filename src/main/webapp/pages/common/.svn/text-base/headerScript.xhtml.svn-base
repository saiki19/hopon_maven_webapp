  <script type="text/javascript" language="javascript">
  	
  	$(document).ready(function() {
  		
  		modalUpdate();
  		  		  		
  		$("div[id^=ui-table-]").each(function(){
  			  			
  			$(this).find("div").each(function(){
  				$(this).removeClass("ui-datatable");
				$(this).removeClass("table-bordered");
  			});

  			$(this).find("tbody").each(function(){
  				$(this).removeClass("ui-widget-content");
			});
  			
  			var testTr = true;
  			$(this).find("th").each(function(){
  				if($(this).find("input:checkbox").length > 0) {
  					testTr = false;
  				} else {  					
  					if($(this).attr("class").indexOf("ui-sortable-column") >= 0) {
  						$(this).attr("class", "ui-sortable-column");
  					} else {
  						$(this).attr("class", "");
  					}
  				}
  			});
			
  			if(testTr) {
				$(this).find("tr").each(function(){
					$(this).attr("class", "");
				});
  			}
			
			$(this).find("table").each(function(){
				$(this).removeClass().addClass("table table-bordered table-striped");
			});
			
			$(this).show();
  		});

  		$("#messagePanel").attr("class" , "");
		$("#messagePanel_content").attr("class" , "");
		$("#messagePanel .alert").each(function(){
			if($(this).find(".form-message").html().trim().length > 0 ) $(this).show();
			else $(this).hide();
		});
  		
  	});
	
  	function modalUpdate() {
  		$("[class^=modal-link-span-]").each(function(){
  			var t = $(this).attr("class").substring($(this).attr("class").indexOf("modal-link-span-") + 16);
  			if(t.indexOf(" ") > 0) t = t.substring(0, t.indexOf(" "));
  			$(this).find("a").attr("data-toggle", "modal");
  			$(this).find("a").attr("href", "#modalPanel-" + t);
  		});
  	}
  	
